<h1>Edit Framework: <%= @frontend.name %></h1>

<form action="/frontends/<%= @frontend.id %>" method="post">

  <label>
    Name:
    <input type="text" name="name" value="<%= @frontend.name %>">
  </label>
  <br>

  <label>
    Date Released:
    <input type="text" name="date_released" value="<%= @frontend.date_released %>">
  </label>
  <br>

  <label>
    Logo:
    <input type="text" name="logo" value="<%= @frontend.logo %>">
  </label>
  <br>

  <input type="submit" value="Edit Framework">

</form>
